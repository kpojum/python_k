def bkc(a,b):
    c=2
    while(True):
        while(c<=9):
            print(a,"*",c,"=",a*c)
            c+=1
        c=2    
        a+=1

        if(a==b):
            while(c<=9):
                print(a,"*",c,"=",a*c)
                c+=1
            break
             
def main():
    a=int(input("첫 번째 숫자를 입력하세요"))
    b=int(input("두 번째 숫자를 입력하세요"))

    if(a > b):
        bkc(b,a)
    else:
        bkc(a,b)


   
main()
